{
    "name": "larabros/laranav",
    "description": "An easy-to-use, customisable menu and navigation package, for Laravel 5.1+",
    "keywords": [
        "larabros",
        "laranav"
    ],
    "homepage": "https://github.com/larabros/laranav",
    "license": "MIT",
    "authors": [
        {
            "name": "Hassan Khan",
            "email": "contact@hassankhan.me",
            "homepage": "http://hassankhan.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "illuminate/support": "^5.1",
        "illuminate/contracts": "^5.1",
        "illuminate/http": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Larabros\\Laranav\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larabros\\Laranav\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
